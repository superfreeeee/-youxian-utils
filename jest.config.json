{
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "testRegex": "/src/(\\.|/)|.*/__tests__/(\\.|/)|.*\\.test\\.(js|jsx|ts|tsx)$",
  "moduleDirectories": ["node_modules", "src"],
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "moduleNameMapper": {
    "^@utils/(.*)": "<rootDir>/src/utils/$1",
    "^@hooks/(.*)": "<rootDir>/src/hooks/$1"
  }
}
